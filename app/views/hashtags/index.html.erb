       
<% @hashtags_tables.each do |tag_table| %>
    <table id="tags">
        <tbody>   
            <% tag_table.each do |tag_row| %>
            <tr>
                <% tag_row.each do |tag| %>
                    <td class="tag-cell">
                        <%=link_to "#{tag.title}", tag_questions_path(tag) %>
                
                    </td>
                <% end %>
            </tr>
            <% end %>
        </tbody>
    </table>
    <br>
<% end %>


            
